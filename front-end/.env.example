# Example Environment Configuration
# Copy this file to .env and modify the values for your setup

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8080/api
REACT_APP_ENVIRONMENT=development

# Optional: WebSocket URL for real-time features
REACT_APP_WS_URL=ws://localhost:8080/ws

# Optional: Enable debug mode (true/false)
REACT_APP_DEBUG=true

# Note: Only variables starting with REACT_APP_ are accessible in React
# For production, update .env.production with your production API URL